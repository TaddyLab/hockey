%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%  Reference Section   %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

%
\newdimen\bibindent
%
%\def\@biblabel#1{#1.}
\renewenvironment{thebibliography}[1]
     {\section*{References}%\bibfont
      \def\arg{#1}%
\ifx\arg\empty
      \list{}%
           {\labelwidth0pt
           \labelsep0pt
            \leftmargin8pt
            \itemindent-\leftmargin
            \itemsep0pt\parsep0pt
            \usecounter{enumiv}%
            \let\p@enumiv\@empty
            \renewcommand\theenumiv{\arabic{enumiv}}}
%
\else      
%
    \list{\@biblabel{\arabic{enumiv}}}%
           {\settowidth\labelwidth{\@biblabel{#1}}%
           \labelsep.5em
            \leftmargin\labelsep
            \advance\leftmargin\labelwidth
            \itemindent0pt\itemsep0pt\parsep0pt
            \usecounter{enumiv}%
            \let\p@enumiv\@empty
            \renewcommand\theenumiv{\arabic{enumiv}}}%
            \fi%
      \sloppy\clubpenalty4000\widowpenalty4000%
      \sfcode`\.=\@m}
     {\def\@noitemerr
       {\@latex@warning{Empty `thebibliography' environment}}%
      \endlist}
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%  End Reference Section   %%%%%%%%%%%%%%%%%%%%%%%%%%%
